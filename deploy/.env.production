# Configuración para VPS Hostinger
NODE_ENV=production
PORT=3001

# Supabase (mantener los valores actuales)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Configuración del dominio
DOMAIN=xistracloud.sinaptiks.com
APPS_DOMAIN=apps.sinaptiks.com

# Docker configuración
DOCKER_SOCKET=/var/run/docker.sock
DOCKER_NETWORK=xistracloud-network

# Configuración de apps
APPS_BASE_PORT=8080
APPS_MAX_PORT=8099

# Configuración de SSL
SSL_ENABLED=true

# Logging
LOG_LEVEL=info
LOG_FILE=/var/log/xistracloud/app.log
